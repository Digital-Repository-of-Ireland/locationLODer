	/* background:url(../images/sprite-bg.png) no-repeat 0 -128px; */
}
.section .box ul a{color:#231f20;}
.img-list{
	padding:10px 0 20px 5px;	margin:0;	list-style:none;
	min-height:261px;
}
.img-list li{
	float:left;
	margin:2px 0 0 -1px;
}
.img-list li.popup-active{position:relative;}
.img-list .popup1{
	z-index:1000;
	position:absolute;
	left:0;
